package loaddata;

public class Skill {
    private String Name; //기술 이름
    private int damageMultiplier;// 기술의 피해량 배율(사용자 공격력 * damageMultiplier = 최종 피해량)
    private String AEffect;// 기술의 부가효과(출혈, 부상, 스텟감소 등)
    private String skillActivationZone;// 기술을 사용 가능한 위치
    private String targetLocation;// 기술이 공격 가능한 위치
    private String Ownertype;// 기술을 사용하는 대상의 범주

    public Skill() {
    }

    public Skill(String name, int damageMultiplier, String aEffect,
                 String skillActivationZone, String targetLocation, String ownertype) {
        this.Name = name;
        this.damageMultiplier = damageMultiplier;
        this.AEffect = aEffect;
        this.skillActivationZone = skillActivationZone;
        this.targetLocation = targetLocation;
        this.Ownertype = ownertype;
    }

    }

    public String getName() {
        return Name;
    }

    public int getDamageMultiplier() {
        return damageMultiplier;
    }

    public String getAEffect() {
        return AEffect;
    }

    public String getSkillActivationZone() {
        return skillActivationZone;
    }

    public String getTargetLocation() {
        return targetLocation;
    }

    public String getOwnertype() {
        return Ownertype;
    }
}